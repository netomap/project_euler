   "source": [
    "- Qual é a soma de todos os números múltiplos de 3 ou 5 até 1000"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "aff3db45",
   "metadata": {},
   "outputs": [
