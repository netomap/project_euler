   ],
   "source": [
    "def multiplo(n: int, fator: int)->bool:\n",
    "    return n % fator == 0\n",
    "\n",
    "soma = 0\n",
    "for n in range(1000):\n",
    "    if (multiplo(n, 3) or multiplo(n, 5)):\n",
    "        soma += n\n",
    "    \n",
    "print (soma)"
   ]
  }
